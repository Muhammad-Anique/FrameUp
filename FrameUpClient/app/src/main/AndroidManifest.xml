        <activity
            android:name=".Activities.HomeVisitor"
            android:exported="false" />
        <activity
            android:name=".Activities.VisitorList"
            android:exported="false" />
        <activity
            android:name=".Activities.Registeration"
            android:exported="false" />
        <activity
            android:name=".Activities.Main"
            android:exported="false" />
    </application>

</manifest>